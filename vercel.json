{
  "version": 2,
  "builds": [
    {
      "src": "frontend",
      "use": "@vercel/static"
    },
    {
      "src": "backend",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ]
}
